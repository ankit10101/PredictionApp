<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.predictionapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.PredictionResultActivity"></activity>
        <activity android:name=".ui.activity.DiseaseDetailActivity" />
        <activity android:name=".ui.activity.DiseasesListActivity" />
        <activity android:name=".ui.activity.ImagePickerActivity" />
        <activity android:name=".ui.activity.MenuActivity" />
        <activity android:name=".ui.activity.UserDetailsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>